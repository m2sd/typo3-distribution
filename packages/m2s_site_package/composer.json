{
  "name": "m2s/site-package",
  "type": "typo3-cms-extension",
  "description": "The main distribution package to configure typo3",
  "license": "GPL-2.0-or-later",
  "version": "0.1.0",
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.1.19"
    }
  },
  "require": {
    "typo3/cms-core": "^8.7",
    "dmitryd/typo3-realurl": "^2.4",
    "helhum/typo3-config-handling": "^0.7",
    "ichhabrecht/content-defender": "^3.0",
    "m2s/typo3-poi-map": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "M2S\\SitePackage\\": "Classes/"
    }
  },
  "replace": {
    "m2s_site_package": "self.version"
  }
}
