        # language alternatives
        20 = HMENU
        20 {
            special = language
            special.value = 0,1
            1 = TMENU
            1 {
                NO {
                    doNotLinkIt = 1
                    stdWrap.cObject = COA
                    stdWrap.cObject {
                        1 = TEXT
                        1 {
                            wrap = <link rel="alternate" hreflang="|"
                            value = it || de
                        }

                        2 = TEXT
                        2 {
                            noTrimWrap = | href="|" />|

                            typolink {
                                parameter.data = page:uid
                                additionalParams = &L=0 || &L=1
                                forceAbsoluteUrl = 1
                                returnLast = url
                            }
                        }
                    }
                }

                ACT = 1
                ACT {
                    doNotLinkIt = 1
                    doNotShowLink = 1
                }
            }
